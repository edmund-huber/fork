import synt
import spring
import utils

entry
  mut argv = spring:args()
  if argv'len != 1
    spring:errln("Wrong number of arguments, required: 1")
    spring:exit(1)
  /if

  mut prs = synt:parserNew(argv'args[0])
  mut root = synt:parserParse(prs)

  if prs'err != null
    utils:issueWriteOut(prs'err, ptr spring:err)
    spring:exit(1)
  /if

  synt:pnodeDump(root)

  synt:parserFree(prs)
  synt:pnodeFree(root)
/entry
